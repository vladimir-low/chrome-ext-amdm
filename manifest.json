{
  "manifest_version": 2,
  "name": "Amdmd tabs searcher",
  "description": "This extension allows the user to search the highly viewed tabs on amdm.ru",
  "version": "0.2",

  "browser_action": {
    "default_icon": "amdm_icon.png",
    "default_popup": "popup.html",
    "default_title": "Search tabs!"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://amdm.ru/*"
  ]
}
